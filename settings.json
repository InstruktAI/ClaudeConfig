{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {},
  "permissions": {
    "allow": [],
    "deny": []
  },
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/tts.py --event-type Notification"
          },
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/send_event.py --source-app teleclaude --event-type Notification --summarize "
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/tts.py --event-type SessionStart"
          },
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/send_event.py --source-app teleclaude --event-type SessionStart"
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/tts.py --event-type SessionEnd"
          },
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/send_event.py --source-app teleclaude --event-type SessionEnd"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/tts.py --event-type Stop"
          },
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type Stop --add-chat"
          },
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/stop.sh"
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/tts.py --event-type SubagentStop"
          },
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type SubagentStop"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/user_prompt_submit.sh"
          },
          {
            "type": "command",
            "command": "$HOME/.claude/.venv/bin/python $HOME/.claude/hooks/send_event.py --source-app teleclaude --event-type UserPromptSubmit --summarize"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bunx -y ccstatusline@latest",
    "padding": 0
  },
  "enabledPlugins": {
    "agent-sdk-dev@claude-code-plugins": true,
    "pr-review-toolkit@claude-code-plugins": true,
    "commit-commands@claude-code-plugins": true,
    "feature-dev@claude-code-plugins": true,
    "security-guidance@claude-code-plugins": true,
    "code-review@claude-code-plugins": true,
    "explanatory-output-style@claude-code-plugins": true,
    "learning-output-style@claude-code-plugins": true,
    "frontend-design@claude-code-plugins": true,
    "hookify@claude-code-plugins": true,
    "plugin-dev@claude-code-plugins": true
  },
  "alwaysThinkingEnabled": true,
  "feedbackSurveyState": {
    "lastShownTime": 1754048693214
  }
}
